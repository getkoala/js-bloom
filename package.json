{
  "name": "@koala-live/js-bloom",
  "author": "Netto Farah",
  "module": "dist/js-bloom.esm.js",
  "private": false,
  "version": "0.1.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepack": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "pub": "npm publish --access=public"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/js-bloom.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/js-bloom.esm.js",
      "limit": "10 KB"
    }
  ],
  "resolutions": {
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.5",
    "husky": "^7.0.4",
    "size-limit": "^7.0.5",
    "timekeeper": "^2.2.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {}
}
